{
  "name": "@aelita-dev/eslint-config",
  "type": "module",
  "version": "3.2.0-beta.2",
  "packageManager": "pnpm@9.9.0",
  "description": "Aelita's ESLint config",
  "author": "Aelita <https://github.com/xsjcTony>",
  "license": "MIT",
  "homepage": "https://github.com/xsjcTony/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/xsjcTony/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "aelita"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npx @eslint/config-inspector --config eslint-config.ts",
    "build": "pnpm typegen && tsup",
    "build:inspector": "pnpm buil && npx @eslint/config-inspector build",
    "watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "typegen": "tsx scripts/typegen.ts",
    "prepack": "pnpm build",
    "release": "bumpp",
    "prepare": "simple-git-hooks",
    "publish-ci": "tsx scripts/publish.ts"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm lint"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": "^14.2.9",
    "@unocss/eslint-plugin": ">=0.62.3 < 1",
    "@vitest/eslint-plugin": "^1.1.0",
    "eslint": "^9.10.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": ">=0.4.11 < 1",
    "eslint-plugin-vue": "^9.28.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-processor-vue-blocks": ">=0.1.2 < 1",
    "vue-eslint-parser": "^9.4.3"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-plugin-vuejs-accessibility": {
      "optional": true
    },
    "eslint-processor-vue-blocks": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-playwright": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.4.1",
    "@clack/prompts": "^0.7.0",
    "@eslint/compat": "^1.1.1",
    "@stylistic/eslint-plugin": "^2.8.0",
    "eslint-config-flat-gitignore": "0.3.0",
    "eslint-flat-config-utils": "^0.4.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^4.2.1",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-unicorn": "^55.0.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "globals": "^15.9.0",
    "local-pkg": "^0.5.0",
    "typescript-eslint": "^8.5.0"
  },
  "devDependencies": {
    "@aelita-dev/eslint-config": "workspace:*",
    "@eslint/config-inspector": "^0.5.4",
    "@next/eslint-plugin-next": "^14.2.9",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20.16.5",
    "@unocss/eslint-plugin": "^0.62.3",
    "@vitest/eslint-plugin": "^1.1.0",
    "bumpp": "^9.5.2",
    "bundle-require": "^5.0.0",
    "changelogithub": "^0.13.10",
    "eslint": "^9.10.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "eslint-plugin-vue": "^9.28.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-processor-vue-blocks": "^0.1.2",
    "eslint-typegen": "^0.3.2",
    "lint-staged": "^15.2.10",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.2.4",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "vue-eslint-parser": "^9.4.3",
    "zx": "^8.1.5"
  }
}
