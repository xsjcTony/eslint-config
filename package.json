{
  "name": "@aelita-dev/eslint-config",
  "type": "module",
  "version": "1.14.4",
  "packageManager": "pnpm@8.15.4",
  "description": "Aelita's ESLint config",
  "author": "Aelita <https://github.com/xsjcTony>",
  "license": "MIT",
  "homepage": "https://github.com/xsjcTony/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/xsjcTony/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "aelita"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "release": "pnpm test:ci && bumpp",
    "typecheck": "tsc --noEmit",
    "preview": "npx @eslint/config-inspector",
    "lint": "eslint .",
    "test:ci": "pnpm typecheck && pnpm build && pnpm lint"
  },
  "peerDependencies": {
    "eslint": ">=8.0.0"
  },
  "dependencies": {
    "@antfu/eslint-define-config": "^1.23.0-2",
    "@unocss/eslint-plugin": "^0.59.0",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-playwright": "^1.6.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-vue": "^9.24.1",
    "eslint-plugin-vuejs-accessibility": "^2.2.1",
    "globals": "^15.0.0",
    "local-pkg": "^0.5.0",
    "typescript-eslint": "^7.6.0",
    "vue-eslint-parser": "^9.4.2"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.4.5",
    "@types/eslint": "^8.56.7",
    "@types/node": "^20.12.7",
    "bumpp": "^9.4.0",
    "bundle-require": "^4.0.2",
    "changelogithub": "^0.13.5",
    "eslint": "^8.57.0",
    "sass": "^1.74.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
