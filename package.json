{
  "name": "@aelita-dev/eslint-config",
  "type": "module",
  "version": "2.1.1",
  "packageManager": "pnpm@9.1.4",
  "description": "Aelita's ESLint config",
  "author": "Aelita <https://github.com/xsjcTony>",
  "license": "MIT",
  "homepage": "https://github.com/xsjcTony/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/xsjcTony/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "aelita"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npx @eslint/config-inspector --config eslint-config.ts",
    "build": "pnpm typegen && tsup",
    "build:inspector": "pnpm buil && npx @eslint/config-inspector build",
    "watch": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "typegen": "tsx scripts/typegen.ts",
    "prepack": "pnpm build",
    "release": "bumpp",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm lint"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": "^14.2.2",
    "@unocss/eslint-plugin": ">=0.59.4 < 1",
    "eslint": ">=8.40.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-playwright": "^1.6.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": ">=0.4.6 < 1",
    "eslint-plugin-vitest": ">=0.5.4 < 1",
    "eslint-plugin-vue": "^9.25.0",
    "eslint-plugin-vuejs-accessibility": "^2.3.0",
    "eslint-processor-vue-blocks": ">=0.1.2 < 1",
    "vue-eslint-parser": "^9.4.2"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-plugin-vuejs-accessibility": {
      "optional": true
    },
    "eslint-processor-vue-blocks": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-playwright": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-vitest": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "@stylistic/eslint-plugin": "^2.1.0",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-flat-config-utils": "^0.2.5",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-antfu": "^2.3.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import-x": "^0.5.1",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-unicorn": "^53.0.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "globals": "^15.3.0",
    "local-pkg": "^0.5.0",
    "typescript-eslint": "^7.11.0"
  },
  "devDependencies": {
    "@aelita-dev/eslint-config": "workspace:*",
    "@eslint/config-inspector": "^0.4.8",
    "@next/eslint-plugin-next": "^14.2.3",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.13",
    "@unocss/eslint-plugin": "^0.60.3",
    "bumpp": "^9.4.1",
    "bundle-require": "^4.1.0",
    "changelogithub": "^0.13.7",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.26.0",
    "eslint-plugin-vuejs-accessibility": "^2.3.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "eslint-typegen": "^0.2.4",
    "lint-staged": "^15.2.5",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "tsx": "^4.11.0",
    "typescript": "^5.4.5",
    "vue-eslint-parser": "^9.4.2"
  }
}
