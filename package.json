{
  "name": "@aelita-dev/eslint-config",
  "type": "module",
  "version": "3.24.1",
  "packageManager": "pnpm@10.15.0",
  "description": "Aelita's ESLint config",
  "author": "Aelita <https://github.com/xsjcTony>",
  "license": "MIT",
  "homepage": "https://github.com/xsjcTony/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/xsjcTony/eslint-config"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "aelita"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npx @eslint/config-inspector --config eslint-config.ts",
    "build": "pnpm typegen && unbuild",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "typegen": "tsx scripts/typegen.ts",
    "prepack": "pnpm build",
    "release": "bumpp",
    "prepare": "simple-git-hooks",
    "publish-ci": "tsx scripts/publish.ts"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "pnpm lint"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": "^15.5.0",
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@tanstack/eslint-plugin-router": "^1.131.2",
    "@unocss/eslint-plugin": ">=66.4.2 <=100",
    "@vitest/eslint-plugin": "^1.3.4",
    "eslint": "^9.34.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-processor-vue-blocks": "^2.0.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-import-x": {
      "optional": true
    },
    "eslint-import-resolver-typescript": {
      "optional": true
    },
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "eslint-plugin-vuejs-accessibility": {
      "optional": true
    },
    "eslint-processor-vue-blocks": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    },
    "eslint-plugin-jsx-a11y": {
      "optional": true
    },
    "eslint-plugin-playwright": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "@tanstack/eslint-plugin-query": {
      "optional": true
    },
    "@tanstack/eslint-plugin-router": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.1.0",
    "@clack/prompts": "^0.11.0",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@typescript-eslint/types": "^8.40.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-flat-config-utils": "^2.1.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-merge-processors": "^2.0.0",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-n": "^17.21.3",
    "eslint-plugin-unicorn": "^60.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "local-pkg": "^1.1.2",
    "typescript-eslint": "^8.40.0"
  },
  "devDependencies": {
    "@aelita-dev/eslint-config": "workspace:*",
    "@eslint/config-inspector": "^1.2.0",
    "@next/eslint-plugin-next": "^15.5.0",
    "@tanstack/eslint-plugin-query": "^5.83.1",
